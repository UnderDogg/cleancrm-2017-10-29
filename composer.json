{
    "name": "cleaniquecoders/splate",
    "description": "Web Application Boilerplate",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "santigarcor/laratrust": "3.0.*",
        "doctrine/dbal": "^2.5",
        "laravel/socialite": "^2.0",
        "tymon/jwt-auth": "0.5.*",
        "laravelcollective/html": "^5.3.0",
        "teepluss/theme": "^2.0",
        "cleaniquecoders/setting": "^1.0",
        "cleaniquecoders/artisan-extended": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "maddhatter/laravel-view-generator": "^0.0.3",
        "nasrulhazim/clean": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Splate\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "php artisan jwt:generate",
            "php artisan clear:cache",
            "php artisan db:setup",
            "rm -fr .git",
            "git init",
            "git add .",
            "git commit -m \"initial commit\""
        ],
        "post-install-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan clear:cache"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
