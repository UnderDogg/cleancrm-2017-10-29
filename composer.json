{
  "name": "cleaniquecoders/splate",
  "description": "Web Application Boilerplate",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "^5.3",
    "doctrine/dbal": "^2.5",
    "laravel/socialite": "^2.0",
    "tymon/jwt-auth": "^0.5",
    "barryvdh/laravel-dompdf": "^0.1",
    "barryvdh/laravel-ide-helper": "^2.3",
    "cebe/markdown": "^1.1",
    "laravie/html": "^5.3.0",
    "teepluss/theme": "^2.0",
    "guzzlehttp/guzzle": "^6.3",
    "intervention/image": "^2.3",
    "jenssegers/date": "^3.2",
    "laracasts/flash": "^3.0",
    "kyslik/column-sortable": "^5.1",
    "nwidart/laravel-menus": "^0.5",
    "nwidart/laravel-modules": "^1.13",
    "laravelcollective/html": "^5.3",
    "sofa/eloquence": "^5.1",
    "tucker-eric/eloquentfilter": "^1.1",
    "rtconner/laravel-tagging": "^2.2",
    "santigarcor/laratrust": "^3.0",
    "thomaswelton/laravel-gravatar": "^1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^0.9",
    "phpunit/phpunit": "^5.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "maddhatter/laravel-view-generator": "^0.0.3"
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "Modules/*/composer.json"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-install-cmd": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan clear:cache"
    ]
  }
}
